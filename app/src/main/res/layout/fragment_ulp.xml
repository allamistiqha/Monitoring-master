<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:gravity="center"
    android:background="#E6E6FA"
    tools:context=".ui.ulp.UlpFragment">
        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_marginBottom="20dp"
            android:layout_height="wrap_content">
            <androidx.appcompat.widget.LinearLayoutCompat
                android:paddingTop="20dp"
                android:paddingBottom="20dp"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:text="ULP Panakukang"
                    android:textAlignment="center"
                    android:textSize="20sp"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:padding="10dp"
                    android:visibility="visible"
                    android:id="@+id/rvPanakukang"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <unhas.informatics.monitoringapp.AviLoading.AVLoadingIndicatorView
                    android:id="@+id/loadingViewPanakukang"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    app:indicatorColor="@color/primary"
                    android:layout_gravity="center"
                    app:indicatorName="LineScaleIndicator" />
                <TextView
                    android:text="ULP Mattoanging"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:textAlignment="center"
                    android:textSize="20sp"/>

                <androidx.recyclerview.widget.RecyclerView
                    android:padding="10dp"
                    android:layout_width="match_parent"
                    android:id="@+id/rvMattoanging"
                    android:layout_height="wrap_content"/>
                <unhas.informatics.monitoringapp.AviLoading.AVLoadingIndicatorView
                    android:id="@+id/loadingViewMattoanging"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    app:indicatorColor="@color/primary"
                    android:layout_gravity="center"
                    app:indicatorName="LineScaleIndicator" />
                <TextView
                    android:text="ULP Sungguminasa"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:textAlignment="center"
                    android:textSize="20sp"/>

                <androidx.recyclerview.widget.RecyclerView
                    android:padding="10dp"
                    android:id="@+id/rvSungguminasa"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <unhas.informatics.monitoringapp.AviLoading.AVLoadingIndicatorView
                    android:id="@+id/loadingViewSungguminasa"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    app:indicatorColor="@color/primary"
                    android:layout_gravity="center"
                    app:indicatorName="LineScaleIndicator" />
                <TextView
                    android:text="ULP Takalar"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:textAlignment="center"
                    android:textSize="20sp"/>

                <androidx.recyclerview.widget.RecyclerView
                    android:padding="10dp"
                    android:id="@+id/rvTakalar"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <unhas.informatics.monitoringapp.AviLoading.AVLoadingIndicatorView
                    android:id="@+id/loadingViewTakalar"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    app:indicatorColor="@color/primary"
                    android:layout_gravity="center"
                    app:indicatorName="LineScaleIndicator" />
                <TextView
                    android:text="ULP Malino"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:textAlignment="center"
                    android:textSize="20sp"/>

                <androidx.recyclerview.widget.RecyclerView
                    android:padding="10dp"
                    android:id="@+id/rvMalino"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <unhas.informatics.monitoringapp.AviLoading.AVLoadingIndicatorView
                    android:id="@+id/loadingViewMalino"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    app:indicatorColor="@color/primary"
                    android:layout_gravity="center"
                    app:indicatorName="LineScaleIndicator" />
                <TextView
                    android:text="ULP Kalebajeng"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:textAlignment="center"
                    android:textSize="20sp"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:padding="10dp"
                    android:id="@+id/rvKalebajeng"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <unhas.informatics.monitoringapp.AviLoading.AVLoadingIndicatorView
                    android:id="@+id/loadingViewKaleBajeng"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    app:indicatorColor="@color/primary"
                    android:layout_gravity="center"
                    app:indicatorName="LineScaleIndicator" />
            </androidx.appcompat.widget.LinearLayoutCompat>
        </androidx.core.widget.NestedScrollView>
</androidx.appcompat.widget.LinearLayoutCompat>